<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.powermetr">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:screenOrientation="portrait"
        android:supportsRtl="true"
        android:theme="@style/Theme.PowerMetr">
        <activity
            android:name=".avtomat3f"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_avtomat3f"
            android:theme="@style/Theme.PowerMetr.NoActionBar"/>
        <activity
            android:name=".avtomat"
            android:label="@string/title_activity_avtomat"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.PowerMetr.NoActionBar" />
        <activity
            android:name=".onefase2"
            android:label="@string/title_activity_onefase2"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.PowerMetr.NoActionBar" />
        <activity
            android:name=".thrifases"
            android:label="@string/title_activity_thrifases"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.PowerMetr.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>